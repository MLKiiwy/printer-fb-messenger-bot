{
  "name": "printer-bot",
  "version": "0.0.0-development",
  "author": "Mikael Labrut",
  "scripts": {
    "analyze": "source-map-explorer dist/static/js/main.*",
    "start:dev": "nodemon src/index.js --exec babel-node",
    "start": "node dist/index.js",
    "prebuild": "rm -rf dist && mkdir dist",
    "preinstall": "cp .env.example .env",
    "postinstall": "yarn prebuild && yarn build",
    "build": "babel -d ./dist ./src",
    "test": "jest --config ./config/jest.config.json --color --runInBand",
    "test:watch": "jest --config ./config/jest.config.json --watch",
    "precommit": "lint-staged",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "commitmsg": "commitlint --config ./config/commitlint.config.js -E GIT_PARAMS",
    "localtunnel": "lt --port 3005"
  },
  "engines": {
    "node": "8.x"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "dependencies": {
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "jest-cucumber": "^1.0.11",
    "jest-junit": "^5.1.0",
    "js-yaml": "^3.12.0",
    "lint-staged": "^7.2.0",
    "localtunnel": "^1.9.1",
    "nock": "^10.0.2",
    "nodemon": "^1.18.6",
    "prettier": "^1.13.5",
    "source-map-explorer": "^1.5.0"
  }
}
